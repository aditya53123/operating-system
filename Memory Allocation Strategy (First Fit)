#include <stdio.h>
#define SIZE 5

int main() {
    int blockSize[SIZE] = {100, 500, 200, 300, 600};
    int processSize[] = {212, 417, 112, 426};
    int allocation[4];

    for (int i = 0; i < 4; i++) allocation[i] = -1;

    for (int i = 0; i < 4; i++) {
        for (int j = 0; j < SIZE; j++) {
            if (blockSize[j] >= processSize[i]) {
                allocation[i] = j;
                blockSize[j] -= processSize[i];
                break;
            }
        }
    }

    for (int i = 0; i < 4; i++) {
        printf("Process %d -> Block ", i+1);
        if (allocation[i] != -1)
            printf("%d\n", allocation[i]+1);
        else
            printf("Not Allocated\n");
    }
    return 0;
}
